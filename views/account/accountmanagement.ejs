

<% if (title) { %>
  <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>

  <% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
  <% } %>

  <h2>Wellcome <%- account_firstname %></h2>

  <p><%- messages() %></p>

  <div>
    <a href="/account/update/<%- account_id %>">Edit Account Information</a>
  </div>

  <% if (link) { %>
    <%- link %>
  <% } %>
  
  
 